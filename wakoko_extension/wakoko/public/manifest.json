{
  "name": "WAKOKO",
  "version": "0.1",
  "manifest_version": 2,

    "content_scripts" : [
      {
        "matches": [ "<all_urls>" ],
        "css": ["/static/css/app.css"],
        "js": ["/static/js/content.js"],
        "run_at":"document_idle"
      }
      
  ], 
   "web_accessible_resources": ["/static/js/app.js", "/static/css/content.css"]
  
}
